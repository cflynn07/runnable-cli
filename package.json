{
  "name": "runnable-cli",
  "version": "0.0.0",
  "description": "A command line interface for the runnable developer sandbox platform",
  "main": "lib/index.js",
  "bin": {
    "runnable": "./lib/cli.js"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "generate-whitelist": "find ./src -name '*_test.js' > ./whitelist.txt",
    "lint": "standard -v src/**/*.js",
    "prepublish": "npm run build",
    "remove-whitelist": "rm ./whitelist.txt",
    "test": "npm run lint; npm run generate-whitelist; istanbul cover lab `cat ./whitelist.txt`; npm run remove-whitelist; istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100",
    "test-html-cov": "npm run generate-whitelist; istanbul cover lab `cat ./whitelist.txt`; npm run remove-whitelist; open -a 'Google Chrome' coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cflynn07/runnable-cli.git"
  },
  "author": "Casey Flynn <cflynn.us@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cflynn07/runnable-cli/issues"
  },
  "homepage": "https://github.com/cflynn07/runnable-cli",
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "code": "^2.1.0",
    "istanbul": "^0.4.1",
    "lab": "^8.0.1",
    "sinon": "^1.17.2",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "101": "^1.2.0",
    "binary-search-insert": "^1.0.2",
    "bluebird": "^3.1.1",
    "cli-spinner": "^0.2.1",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "keypather": "^1.10.1",
    "keypress": "^0.2.1",
    "moment": "^2.11.0",
    "node-watch": "^0.3.5",
    "open": "0.0.5",
    "primus": "^4.0.4",
    "qs": "^6.0.1",
    "request": "^2.67.0",
    "seamless-immutable": "^4.1.1",
    "simple-git": "git://github.com/CFlynn07/git-js.git#97ba9d585be78e370311a33c1dd27b6fe09cb9d1",
    "substream": "^0.2.0",
    "template-string": "^1.1.0",
    "uuid": "^2.0.1",
    "ws": "^1.0.0"
  }
}
